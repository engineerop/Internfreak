   <!doctype html>
<html lang="en">
  <head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">

    <title>InternFreak</title>
  </head>
  <body>
  
      <div class="container">
       <h1 class="mb-4">New Article</h1>
       <form action ="/articles" method="POST">
        <form>
            
            <div class="form-outline mb-4">
              <input required type="text" id="title" name="title"class="form-control" <%= article.title %>  />
              <label class="form-label" for="title">Title</label>
            </div>
            <div class="form-outline mb-4">
                <input required type="text" id="category" name="category" <%= article.category %>/>
                <label class="form-label" for="category">Category</label>
              </div>
              <!-- <div>
                <label for="image">Upload Image</label>
                <input type="file" id="image"
                       name="image" value="" required <%= article.image%>/>
            </div> -->
            <!-- Email input -->
            <div class="form-outline mb-4">
              <input required type="text" id="description" name="description" class="form-control"<%= article.description %> />
              <label class="form-label" for="description">Description</label>
            </div>
            <div class="form-outline mb-4">
              <input required type="text" id="companyDetails" name="companyDetails" class="form-control"<%= article.companyDetails %> />
              <label class="form-label" for="companyDetails">companyDetails</label>
            </div>
            <div class="form-outline mb-4">
              <input required type="text" id="heading" name="heading" class="form-control"<%= article.heading %> />
              <label class="form-label" for="heading">heading</label>
            </div>
            <div class="form-outline mb-4">
              <input required type="text" id="designation" name="designation" class="form-control"<%= article.designation %> />
              <label class="form-label" for="designation">designation</label>
            </div>
            <div class="form-outline mb-4">
              <input required type="text" id="qualification" name="qualification" class="form-control"<%= article.qualification %> />
              <label class="form-label" for="qualification">qualification</label>
            </div>
            <div class="form-outline mb-4">
              <input required type="text" id="ctc" name="ctc" class="form-control"<%= article.ctc %> />
              <label class="form-label" for="ctc">ctc</label>
            </div>
            <div class="form-outline mb-4">
              <input required type="text" id="location" name="location" class="form-control"<%= article.location %> />
              <label class="form-label" for="location">location</label>
            </div>
            <div class="form-outline mb-4">
              <input required type="text" id="applyLink" name="applyLink" class="form-control"<%= article.applyLink %> />
              <label class="form-label" for="applyLink">applyLink</label>
            </div>
            <div class="form-outline mb-4">
              <input required type="text" id="candidateShouldHave" name="candidateShouldHave" class="form-control"<%= article.candidateShouldHave %> />
              <label class="form-label" for="candidateShouldHave">candidateShouldHave</label>
            </div>
            <!-- <div class="form-outline mb-4">
              <input type="text" id="form4Example2" class="form-control" />
              <label class="form-label" for="form4Example2">Created At</label>
            </div> -->
          
            <!-- Message input -->
            <!-- <div class="form-outline mb-4">
              <input required type="text" id="writtenBy" name="writtenBy" class="form-control"<%= article.writtenBy %> />
              <label class="form-label" for="writtenBy">writtenBy</label>
            </div> -->
          
            <!-- Submit button -->
            <button type="submit" class="btn btn-primary btn-block mb-4">Send</button>
          </form>
       </form>
   </div> 
    <!-- Optional JavaScript -->
    <!-- jQuery first, then Popper.js, then Bootstrap JS -->
    <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js" integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js" integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM" crossorigin="anonymous"></script>
  </body>
</html>